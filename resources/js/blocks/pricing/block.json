{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "imagewize/pricing",
    "version": "0.2.0",
    "title": "Pricing Table (Core Blocks)",
    "category": "layout",
    "icon": "table-col-after",
    "description": "Display pricing options using nested core blocks like Group, Columns, Headings, and Paragraphs.",
    "example": {},
    "supports": {
        "html": false,
        "align": ["wide", "full"],
        "color": {
            "background": true,
            "text": false,
            "link": false
        },
        "spacing": {
            "padding": true,
            "margin": true
        }
    },
    "textdomain": "imagewize",
    "style": "file:./style.css",
    "editorStyle": "file:./editor.css",
    "viewScript": "file:./view.js",
    "attributes": {
        "className": {
            "type": "string",
            "default": "wp-block-imagewize-pricing"
        }
    }
}