{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "imagewize/feature-list-grid",
    "version": "0.1.0",
    "title": "Feature List Grid",
    "category": "imagewize",
    "icon": "grid-view",
    "description": "Display features in a 2-column grid with checkmarks and hover effects.",
    "example": [],
    "supports": {
        "html": false,
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": true
        },
        "color": {
            "background": true,
            "text": true
        }
    },
    "textdomain": "imagewize",
    "style": "file:./style.css",
    "editorStyle": "file:./editor.css",
    "viewScript": "file:./view.js",
    "attributes": {
        "className": {
            "type": "string",
            "default": "wp-block-imagewize-feature-list-grid"
        },
        "align": {
            "type": "string",
            "default": "wide"
        }
    }
}